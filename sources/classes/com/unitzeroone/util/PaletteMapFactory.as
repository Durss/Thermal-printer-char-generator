package com.unitzeroone.util {	import com.unitzeroone.fx.data.PaletteMap;				/**	 * @author Ralph Hauwert	 */	public class PaletteMapFactory	{				public static function createPosterizeMap(levels:int):PaletteMap		{			var rA:Array = new Array();			var gA:Array = new Array();			var bA:Array = new Array();			var i:int;			var lNorm:Number = 255/levels;			var v:int;			for(i=0; i<=255;i++){				v = Math.round((i/255) * levels) * lNorm;				rA[i] = v << 16;				gA[i] = v << 8;				bA[i] = v;			}			trace(rA);			return new PaletteMap(rA, gA, bA, null);		}			}}